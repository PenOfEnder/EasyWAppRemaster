---
interface Props {
    size: string;
    color: string;
}

const { size, color } = Astro.props;

---

<svg
    xmlns="http://www.w3.org/2000/svg"
    viewBox="0 0 24 24"
    fill="none"
    stroke={color}
    stroke-linecap="round"
    stroke-linejoin="round"
    width={size}
    height={size}
    stroke-width="2"
>
    <path d="M3 5v14l8 -7z"></path>
    <path d="M14 5v14l8 -7z"></path>
</svg>

