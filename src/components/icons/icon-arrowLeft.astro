---
    interface Props{
        size?: string,
        color?: string,
        stroke?: string,
    }

    const { size, color, stroke = "1.5" } = Astro.props
---


<svg
    xmlns="http://www.w3.org/2000/svg"
    width={size}
    height={size}
    viewBox="0 0 24 24"
    ><path
        fill="none"
        stroke={color}
        stroke-linecap="round"
        stroke-linejoin="round"
        stroke-width={stroke}
        d="m12 5l-7 7m0 0l7 7m-7-7h14"></path></svg
>
