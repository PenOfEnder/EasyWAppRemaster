---

interface Props{
    size: string
    color: string
}

const { size, color }: Props = Astro.props;

---

<svg
    xmlns="http://www.w3.org/2000/svg"
    width={size}
    height={size}
    viewBox="0 0 24 24"
    ><g
        fill="none"
        stroke={color}
        stroke-linecap="round"
        stroke-linejoin="round"
        stroke-width="2"
        ><path d="M20 7h-9m3 10H5"></path><circle cx="17" cy="17" r="3"
        ></circle><circle cx="7" cy="7" r="3"></circle></g
    ></svg
>
